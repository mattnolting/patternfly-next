<td{{#if table-td--toggle}} class="pf-c-table__toggle"
    {{else if table-td--check}} class="pf-c-table__check"
    {{else if table-td--action}} class="pf-c-table__action"
    {{else if table-td--icon}} class="pf-c-table__icon"
    {{else if table-td--compound-expansion-toggle}} class="pf-c-table__compound-expansion-toggle{{#if table-td--compound-expansion-toggle--IsExpanded}} pf-m-expanded{{/if}}{{table-td--modifier}}"
    {{else if table-td--action}} class="pf-c-table__action"
    {{else if table-td--modifier}} class="{{table-td--modifier}}"
    {{/if}}
    {{#unless table-td--IsEmpty}}
      role="cell"
    {{/unless}}
  {{#if table-td--attribute}}
    {{{table-td--attribute}}}
  {{/if}}
  {{#if table-td--data-label}}
    data-label="{{table-td--data-label}}"
  {{/if}}>
  {{#if table-td--toggle}}
    <button class="pf-c-button pf-m-plain{{#if table-tr--expanded}} pf-m-expanded{{/if}}" {{#if table-td--button--attribute}}{{{table-td--button--attribute}}}{{/if}} {{#if table-tr--expanded}}aria-expanded="true"{{/if}}>
      {{#> table-toggle-icon}}{{/table-toggle-icon}}
    </button>
  {{else if table-td--compound-expansion-toggle}}
    {{#if table-td--compound-expansion-toggle--IsExpanded}}
      {{#> button button--modifier="pf-m-link" button--attribute=(concat 'aria-expanded="true" aria-controls="' table--id '-' table-td--compound-expansion-toggle--button--aria-controls '"' table-td--compound-expansion-toggle--button--attribute)}}
        {{#> table-cell-content}}
          {{#> table-text}}
            {{> @partial-block}}
          {{/table-text}}
        {{/table-cell-content}}
      {{/button}}
    {{else}}
      {{#> button button--modifier="pf-m-link" button--attribute=(concat 'aria-expanded="false" aria-controls="' table--id '-' table-td--compound-expansion-toggle--button--aria-controls '"' table-td--compound-expansion-toggle--button--attribute)}}
        {{#> table-cell-content}}
          {{#> table-text}}
            {{> @partial-block}}
          {{/table-text}}
        {{/table-cell-content}}
      {{/button}}
    {{/if}}
  {{else if table-tr--expandable}}
    {{#> table-expandable-row-content}}
      {{> @partial-block}}
    {{/table-expandable-row-content}}
  {{else if table-td--check}}
    {{> @partial-block}}
  {{else if table-td--action}}
    {{> @partial-block}}
  {{else if table-td--icon}}
    {{> @partial-block}}
  {{else if table-td--action}}
    {{> @partial-block}}
  {{else if table-td--IsCustom}}
    {{> @partial-block}}
  {{else if table-td--IsEmpty}}
    {{> @partial-block}}
  {{else}}
    {{#> table-cell-content}}
      {{#> table-text}}
        {{> @partial-block}}
      {{/table-text}}
    {{/table-cell-content}}
  {{/if}}
</td>
